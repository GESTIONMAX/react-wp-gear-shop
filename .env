# Production environment variables
# These should be configured on Vercel for production deployment
# For local development, use .env.local (gitignored)

VITE_SUPABASE_PROJECT_ID="your_project_id"
VITE_SUPABASE_URL="https://your_project_id.supabase.co"
VITE_SUPABASE_PUBLISHABLE_KEY="your_supabase_anon_key"

# Database connections (template values)
DATABASE_URL="postgresql://postgres:password@localhost:5432/database_name"
DIRECT_URL="postgresql://postgres:password@localhost:5432/database_name"

# Stripe configuration (use your own keys)
VITE_STRIPE_PUBLISHABLE_KEY="pk_test_your_stripe_publishable_key"
STRIPE_SECRET_KEY="sk_test_your_stripe_secret_key"